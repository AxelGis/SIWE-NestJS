{
  "name": "pp-site-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "//--start-scripts": "SCRIPTS FOR START APP",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:devdb": "npm run migrate:devdeploy && npm run seed:dev && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "//--format-lint-scripts": "SCRIPTS FOR PREPARE AND CHECK CODE",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "//--test-scripts": "SCRIPTS FOR RUN TESTS",
    "test": "jest",
    "test:cov": "jest",
    "test:unit": "jest",
    "//--prisma-scripts": "SCRIPTS FOR PRISMA",
    "//--prisma-gen-scripts": "SCRIPTS FOR GENERATE PRISMA CLIENT",
    "prisma:gen": "prisma generate --schema=.ops/prisma/schema.prisma",
    "//--prisma-seed-scripts": "SCRIPTS FOR RUN SEEDS",
    "seed:dev": "node_modules/.bin/dotenv -e .ops/.env/.env.dev -e .ops/.env/.env.secrets -- ts-node .ops/prisma/seed.ts --env dev",
    "seed:devclean": "node_modules/.bin/dotenv -e .ops/.env/.env.dev -e .ops/.env/.env.secrets -- ts-node .ops/prisma/seed.ts --env dev --clean",
    "seed:e2e": "node_modules/.bin/dotenv -e .ops/.env/.env.e2e -e .ops/.env/.env.secrets -- ts-node .ops/prisma/seed.ts --env dev",
    "seed:staging": "ts-node .ops/prisma/seed.ts --env prod",
    "seed:production": "ts-node .ops/prisma/seed.ts --env prod",
    "//--prisma-migrate-scripts": "SCRIPTS FOR RUN MIGRATIONS",
    "migrate:dev": "node_modules/.bin/dotenv -e .env -e .ops/.env/.env.dev -e .ops/.env/.env.secrets -- prisma migrate dev --schema .ops/prisma/schema.prisma",
    "migrate:devdeploy": "node_modules/.bin/dotenv -e .env -e .ops/.env/.env.dev -e .ops/.env/.env.secrets -- prisma migrate deploy --schema .ops/prisma/schema.prisma",
    "migrate:e2e": "node_modules/.bin/dotenv -e .ops/.env/.env.e2e -e .ops/.env/.env.secrets -- prisma migrate dev --schema .ops/prisma/schema.prisma",
    "migrate:staging": "prisma migrate deploy --schema .ops/prisma/schema.prisma",
    "migrate:production": "prisma migrate deploy --schema ./prisma/schema.prisma",
    "//--language-server-scripts": "SCRIPTS FOR RUN LANGUAGE-SERVER",
    "ls": "npm run start --workspace language-server",
    "ls:dev": "npm run dev --workspace language-server"
  },
